        <sonar.findbugs.timeout>6000000</sonar.findbugs.timeout>
        <!-- ******************************************************************************************* -->
        <!-- END Sonar Properties -->
        <!-- ******************************************************************************************* -->
        <xz.version>1.8</xz.version>

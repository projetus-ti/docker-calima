    <!-- Construção -->
    <build>
        <finalName>${project.parent.artifactId}</finalName>
        <plugins>
            <plugin>
                <artifactId>maven-antrun-plugin</artifactId>
                <version>${ant.plugin.version}</version>
                <executions>
                    <execution>
                        <id>flex-build</id>
                        <phase>compile</phase>
                        <configuration>
                            <tasks>
                                <propertyfile file="../flex/build.properties">
                                    <entry key="project.version" value="${project.version}"/>
                                    <entry key="build.number" value="${buildNumber}"/>
                                </propertyfile>
                                <ant antfile="../flex/build.xml" usenativebasedir="true">
                                    <target name="build"/>
                                </ant>
                            </tasks>
                        </configuration>
                        <goals>
                            <goal>run</goal>
                        </goals>
                    </execution>

                    <execution>
                        <id>archive</id>
                        <phase>install</phase>
                        <configuration>
                            <tasks>
                                <mkdir dir="C:/qa/homologacao/calima/${project.version}"/>
                                <copy todir="C:/qa/homologacao/calima/${project.version}"
                                      overwrite="true">
                                    <fileset dir="${project.build.directory}">
                                        <include name="${project.parent.artifactId}.war"/>
                                    </fileset>
                                </copy>

                                <move file="C:/qa/homologacao/calima/${project.version}/${project.parent.artifactId}.war"
                                      tofile="C:/qa/homologacao/calima/${project.version}/${project.parent.artifactId}-${project.version}-${buildNumber}.war"/>

                                <ant antfile="../flex/build.xml">
                                    <target name="update-release-list"/>
                                </ant>
                            </tasks>
                        </configuration>
                        <goals>
                            <goal>run</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <!-- Plugin para geração do número do build -->
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>buildnumber-maven-plugin</artifactId>
                <version>${buildnumber.plugin.version}</version>
                <executions>
                    <execution>
                        <phase>validate</phase>
                        <goals>
                            <goal>create</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <providerImplementations>
                        <svn>javasvn</svn>
                    </providerImplementations>
                    <doCheck>false</doCheck>
                    <doUpdate>false</doUpdate>

                    <format>{0,date,yyMMddHH}</format>
                    <items>
                        <item>timestamp</item>
                    </items>
                </configuration>
            </plugin>

            <!-- Plugin de compilação para os relatórios -->
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>jasperreports-maven-plugin</artifactId>
                <version>${jasperreports.plugin.version}</version>
                <configuration>
                    <compiler>net.sf.jasperreports.compilers.JRGroovyCompiler</compiler>
                    <!--outputDirectory>${project.build.outputDirectory}/../calima/WEB-INF/classes</outputDirectory-->
                </configuration>
                <executions>
                    <execution>
                        <phase>compile</phase>
                        <id>default-cli</id>
                        <goals>
                            <goal>compile-reports</goal>
                        </goals>
                        <configuration>
                            <sourceDirectory>src/main/groovy</sourceDirectory>
                        </configuration>
                    </execution>
                    <execution>
                        <phase>compile</phase>
                        <id>compile-java</id>
                        <goals>
                            <goal>compile-reports</goal>
                        </goals>
                        <configuration>
                            <sourceDirectory>src/main/java</sourceDirectory>
                        </configuration>
                    </execution>
                </executions>
                <dependencies>

                    <dependency>
                        <groupId>net.sf.jasperreports</groupId>
                        <artifactId>jasperreports</artifactId>
                        <version>${jasperreports.version}</version>
                    </dependency>

                    <dependency>
                        <groupId>org.codehaus.groovy</groupId>
                        <artifactId>groovy-all</artifactId>
                        <version>${groovy.version}</version>
                    </dependency>

                    <dependency>
                        <groupId>log4j</groupId>
                        <artifactId>log4j</artifactId>
                        <version>${log4j.version}</version>
                    </dependency>

                    <dependency>
                        <groupId>net.sf.barcode4j</groupId>
                        <artifactId>barcode4j</artifactId>
                        <version>${barcode4j.version}</version>
                    </dependency>

                </dependencies>
            </plugin>

            <plugin>
                <groupId>com.samaxes.maven</groupId>
                <artifactId>minify-maven-plugin</artifactId>
                <version>1.7.6</version>
                <executions>
                    <execution>
                        <id>minify-js</id>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>minify</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <jsEngine>CLOSURE</jsEngine>
                    <webappTargetDir>${project.build.directory}/min</webappTargetDir>
                    <charset>utf-8</charset>
                    <skipMerge>false</skipMerge>
                    <nosuffix>true</nosuffix>
                    <jsFinalFile>calima.js</jsFinalFile>
                    <cssFinalFile>calima.css</cssFinalFile>
                    <cssSourceIncludes>
                        <cssSourceInclude>**</cssSourceInclude>
                    </cssSourceIncludes>
                    <cssSourceExcludes>
                        <cssSourceExclude>**/*.min.css</cssSourceExclude>
                    </cssSourceExcludes>
                    <jsSourceIncludes>
                        <jsSourceInclude>**</jsSourceInclude>
                    </jsSourceIncludes>
                    <jsSourceExcludes>
                        <jsSourceExclude>**/*.min.js</jsSourceExclude>
                    </jsSourceExcludes>
                </configuration>
            </plugin>
            <plugin>
                <artifactId>maven-war-plugin</artifactId>
                <configuration>
                    <webResources>
                        <resource>
                            <directory>${project.build.directory}/min</directory>
                        </resource>
                    </webResources>
                </configuration>
            </plugin>
        </plugins>
    </build>

